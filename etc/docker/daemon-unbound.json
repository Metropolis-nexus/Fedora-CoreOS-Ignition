{
    "default-runtime": "runsc-systrap",
    "runtimes": {
        "runsc-systrap": {
            "path": "/usr/local/bin/runsc",
            "runtimeArgs": [
                "--platform=systrap",
                "--network=host"
            ]
        },
        "runsc-systrap --host-uds=open": {
            "path": "/usr/local/bin/runsc",
            "runtimeArgs": [
                "--platform=systrap",
                "--network=host",
                "--host-uds=open"
            ]
        }
    },
    "default-address-pools": [
        { "base": "242.242.0.0/16", "size": 24 }
    ],
    "dns" : [ "242.242.0.1" ]
}